import java.util.*;
class Fibonacci
{
    public static void main(String arg[])
     {
       int n1=0,n2=1,n3,n,i;
       Scanner sc=new Scanner(System.in);
       System.out.println("Enter the n value:");
       n=sc.nextInt();
       for(i=2;i<=n;i++)
        { 
            n3=n1+n2;
            System.out.println("+n3");
            n1=n2;
            n2=n3;
        }
      System.out.println(n3);
     }
}
